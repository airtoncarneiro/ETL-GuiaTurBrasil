{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python Debugger: Current File",
			"type": "debugpy",
			"request": "launch",
			"program": "${file}",
			"console": "integratedTerminal",
			"env": {
				"PYTHONPATH": "${workspaceFolder}/src/lambda/cidades:${workspaceFolder}/src/layer:${workspaceFolder}/src/layer/custom:${workspaceFolder}/.aws-sam/build/LibsLayer"
			}
		}
	],
	"settings": {
		"python.analysis.extraPaths": [
			"${workspaceFolder}/src/layer",
			"${workspaceFolder}/src/layer/custom",
			"${workspaceFolder}/.aws-sam/build/LibsLayer"
		]
	}
}