{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python Debugger: Current File",
			"type": "debugpy",
			"request": "launch",
			"program": "${file}",
			"console": "integratedTerminal",
			"env": {
				"PYTHONPATH": "${workspaceFolder}/src/lambda/cidades:${workspaceFolder}/src/layer:${workspaceFolder}/src/layer/custom:${workspaceFolder}/.aws-sam/build/LibsLayer",
			}
		}
	]
}